<requires>
  <import addon="xbmc.addon" version="12.0.0"/>
</requires>
	<extension point="xbmc.addon.repository" name="Ru Users addons">
		<info compressed="false">https://github.com/usersru/ruka/raw/master/addons.xml</info>
		<checksum>https://github.com/usersru/ruka/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/usersru/ruka/raw/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
